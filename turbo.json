{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env.*local"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"check": {
			"dependsOn": ["^check"]
		},
		"sb": {
			"dependsOn": ["^sb"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
